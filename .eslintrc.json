{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "ignorePatterns": ["projects/**/*"],
  "rules": {
    "jsdoc/check-indentation": "off",
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": [
      "error",
      { "argsIgnorePattern": "^_" }
    ]
  },
  "globals": {
    "NodeJS": true,
    "Express": true,
    "Mocha": true
  },
  "overrides": []
}
