{
  "name": "upload-server",
  "description": "TPUv2 Server",
  "version": "1.0.0",
  "private": true,
  "authors": [
    "Troplo <troplo@troplo.com>",
    "The Loose Goose"
  ],
  "scripts": {
    "start": "nodemon",
    "serve": "tsc-watch --project tsconfig.app.json --onSuccess 'node ./out/index.js'",
    "build": "rimraf out && tsc --project tsconfig.app.json",
    "format": "prettier --config ../.prettierrc.yaml --write \"app/**/*\"",
    "lint": "eslint -c .eslintrc.json \"./app/**/*.ts\" --format stylish",
    "test": "cross-env TS_NODE_PROJECT='./tsconfig.spec.json' mocha --config=.mocharc.json --node-env=test",
    "coverage": "cross-env TS_NODE_PROJECT='./tsconfig.spec.json' nyc mocha --config=.mocharc.json --node-env=test --exit"
  },
  "nyc": {
    "check-coverage": true,
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80,
    "per-file": true,
    "include": [
      "app/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.spec.ts",
      "app/index.ts",
      "app/server.ts",
      "app/app.ts"
    ],
    "require": [
      "ts-node/register",
      "tsconfig-paths/register"
    ],
    "reporter": [
      "html",
      "text",
      "cobertura"
    ],
    "all": true,
    "cache": true,
    "sourceMap": true,
    "instrument": true
  },
  "_moduleAliases": {
    "@app": "out"
  },
  "dependencies": {
    "@levminer/speakeasy": "^1.4.0",
    "argon2": "^0.30.3",
    "bullmq": "^3.5.6",
    "cache-manager": "^5.1.4",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "crypto-random-string": "3.3.1",
    "dayjs": "^1.11.7",
    "debug": "4.3.4",
    "express": "4.18.1",
    "express-promise-router": "^4.1.1",
    "http-status-codes": "2.1.4",
    "husky": "^8.0.3",
    "jw-paginate": "^1.0.4",
    "mariadb": "^3.0.2",
    "module-alias": "^2.2.2",
    "multer": "^1.4.5-lts.1",
    "node-tesseract-ocr": "^2.2.1",
    "prettier": "2.7.1",
    "redis": "^4.5.1",
    "reflect-metadata": "0.1.13",
    "sequelize": "^6.28.0",
    "sequelize-pool": "8.0.0",
    "sequelize-typescript": "^2.1.5",
    "serve-favicon": "2.5.0",
    "socket.io": "^4.5.4",
    "swagger-jsdoc": "6.2.5",
    "swagger-ui-express": "4.5.0",
    "typedi": "0.10.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.1",
    "@types/chai": "4.3.3",
    "@types/cookie-parser": "1.4.3",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/mocha": "9.1.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.18",
    "@types/node-uuid": "0.0.29",
    "@types/sequelize": "^4.28.14",
    "@types/sinon": "10.0.13",
    "@types/supertest": "2.0.11",
    "@types/swagger-jsdoc": "6.0.1",
    "@types/swagger-ui-express": "4.1.3",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "5.32.0",
    "@typescript-eslint/parser": "5.32.0",
    "chai": "4.3.6",
    "cross-env": "7.0.3",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.4.0",
    "eslint-plugin-deprecation": "1.3.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.3.4",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-unicorn": "43.0.2",
    "mocha": "^10.2.0",
    "nodemon": "2.0.19",
    "nyc": "15.1.0",
    "rimraf": "3.0.2",
    "sinon": "14.0.0",
    "source-map-support": "0.5.21",
    "supertest": "6.2.4",
    "ts-node": "10.9.1",
    "tsc-watch": "^6.0.0",
    "tsconfig-paths": "4.1.2",
    "typescript": "^4.9.4"
  }
}
